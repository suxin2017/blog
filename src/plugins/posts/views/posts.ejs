<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">名称</th>
        <th scope="col">描述</th>
        <th scope="col">状态</th>
      </tr>
    </thead>
    <tbody>
        <% for( let post of posts) { %>
            <tr>
                <th scope="row"><%= post.id %> </th>
                <td><%= post.name %> </td>
                <td><%= post.deescription %> </td>
                <td><%= post.views %></td>
              </tr>
        <% } %>
    </tbody>
  </table>

